{
  "name": "bike-ride-bot",
  "version": "2.0.1",
  "description": "Telegram bot for organizing bike rides",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "MONGOMS_DOWNLOAD_DIR=/tmp/mongodb-binaries node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000",
    "test:coverage": "MONGOMS_DOWNLOAD_DIR=/tmp/mongodb-binaries node --experimental-vm-modules node_modules/jest/bin/jest.js --testTimeout=30000 --coverage"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.2.0",
    "cheerio": "^1.0.0-rc.12",
    "chrono-node": "^2.7.0",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "grammy": "^1.21.1",
    "mongoose": "^8.1.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.1"
  },
  "type": "module",
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/storage/interface.js"
    ]
  }
}
